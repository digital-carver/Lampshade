<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="SeekBarPreference">
        <attr name="minValue" format="integer"></attr>
        <attr name="maxValue" format="integer"></attr>
        <attr name="units" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="customAttrs">
        <attr name="randomIcon" format="reference" />
        <attr name="saveIcon" format="reference" />
        <attr name="deleteIcon" format="reference" />
        <attr name="favoriteIcon" format="reference" />
        <attr name="unfavoriteIcon" format="reference" />
    </declare-styleable>

</resources>